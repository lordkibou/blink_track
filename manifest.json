{
    "manifest_version": 3,
    "name": "Blinktrack",
    "version": "1.0",
    "description": "Save your eyes by tracking your blink",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
        "activeTab",
        "storage"
      ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background/background.js"
    },
    "options_page": "options/options.html"
  }
  